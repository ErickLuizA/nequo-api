<td class='p-5 rounded-md' up-expand>
  @if(isArray)
  @each(val in value)
  <a class="text-lg" up-layer="new" up-mode="cover" up-class="up-modal-container" href="{{ href }}">{{ val }}</a>
  @endeach
  @else
  <a class="text-lg" up-layer="new" up-mode="cover" up-class="up-modal-container" href="{{ href }}">{{ value }}</a>
  @endif
</td>